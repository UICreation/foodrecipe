{
  "name": "food-recipe-app",
  "version": "1.0.0",
  "description": "Food Recipe app built in Node and React Js",
  "scripts": {
    "build": "webpack",
    "start": "webpack-dev-server",
    "lint": "npx eslint ./src"
  },
  "lint-staged": {
    "*.js": [
      "npx eslint ./src",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
  },
  "keywords": [
    "food recipe app"
  ],
  "author": "Shivam Kumar",
  "homepage": "https://github.com/UICreation/foodrecipe",
  "bugs": {
    "url": "https://github.com/UICreation/foodrecipe/issues",
    "email": "shivam.designweb@gmail.com"
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "babel-loader": "^8.0.6",
    "basscss-background-colors": "^2.1.0",
    "basscss-colors": "^2.2.0",
    "css-loader": "^3.4.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.0.6",
    "lint-staged": "^9.5.0",
    "postcss": "^7.0.26",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "style-loader": "^1.1.2",
    "to-string-loader": "^1.1.6",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "basscss": "^8.1.0",
    "sanitize.css": "^11.0.0",
    "request": "^2.88.0"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "mongodb": "^3.5.2",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "whatwg-fetch": "^3.0.0"
  }
}
